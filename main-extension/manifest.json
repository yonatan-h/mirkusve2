{
	"manifest_version": 3,
	"version": "1.0",
	"name": "Mirkusve2",
	"description": "measure how much you lose",
	"content_scripts": [
		{
			"matches": ["https://leetcode.com/problems/*/description/"],
			"js": ["timer/timer.js"]
		}
	],
	"permissions": ["storage"],

	"web_accessible_resources": [
		{
			"resources": ["timer/timer.html"],
			"matches": ["<all_urls>"]
		}
	]
}
